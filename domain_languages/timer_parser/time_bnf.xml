<time-12-hour-format> ::= <hour-part> <day-night-part> <EOL> |
    <hour-part> ":" <minute-part> <day-night-part> <EOL> |
    <hour-part> ":" <minute-part> <EOL>

<time-24-hour-format> ::= <hour-part> ":" <minute-part>